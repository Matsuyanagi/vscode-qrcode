{
  "name": "vscode-qrcode",
  "displayName": "QR Code Generator",
  "description": "Generates QR code and save as transparent GIF",
  "version": "0.2.0",
  "publisher": "Compulim",
  "engines": {
    "vscode": "^1.39.0"
  },
  "icon": "icon.png",
  "galleryBanner.color": "#0D5CAB",
  "keywords": [
    "qr",
    "qr code"
  ],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/compulim/vscode-qrcode.git"
  },
  "bugs": "https://github.com/compulim/vscode-qrcode/issues/",
  "homepage": "https://github.com/compulim/vscode-qrcode/blob/master/README.md",
  "activationEvents": [
    "onCommand:qrcode.generateFromSelected"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qrcode.generateFromSelected",
        "title": "QR code: Generate QR code from text"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.39.0"
  },
  "dependencies": {
    "q": "^1.5.1",
    "qrcode-generator": "^1.4.4"
  }
}
